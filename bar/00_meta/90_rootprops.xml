                <p:service language="JavaScript" subject="me" xmlns:D="DAV:" xmlns:p="urn:x-personium:xmlns" xmlns:Z="http://www.w3.com/standards/z39.50/">
                    <p:path name="setTimer" src="setTimer.js"/>
                </p:service>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/SetTimer/__src</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                </resourcetype>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/SetTimer/__src/setTimer.js</href>
        <propstat>
            <prop>
                <getcontenttype>text/javascript</getcontenttype>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/NotifySchedule</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                    <p:service xmlns:p="urn:x-personium:xmlns"/>
                </resourcetype>
                <p:service language="JavaScript" subject="me" xmlns:D="DAV:" xmlns:p="urn:x-personium:xmlns" xmlns:Z="http://www.w3.com/standards/z39.50/">
                    <p:path name="notifySchedule" src="notifySchedule.js"/>
