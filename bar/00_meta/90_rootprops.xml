<multistatus xmlns="DAV:">
    <response>
        <href>dcbox:/</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                </resourcetype>
                <acl xml:base="https://demo.personium.io/app-schedule-notifier/__role/__/" xmlns:dc="urn:x-dcl:xmlns">
                    <ace>
                        <principal>
                            <href>ReminderRole</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                            <privilege>
                                <write/>
                            </privilege>
                            <privilege>
                                <exec/>
                            </privilege>
                        </grant>
                    </ace>
                </acl>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/TimerList</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
