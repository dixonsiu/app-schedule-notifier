    <response>
        <href>dcbox:/NotifySchedule/__src/notifySchedule.js</href>
        <propstat>
            <prop>
                <getcontenttype>text/javascript</getcontenttype>
            </prop>
        </propstat>
    </response>
    <response>
      <href>dcbox:/ServiceInfo</href>
      <propstat>
          <prop>
              <resourcetype>
                  <collection/>
              </resourcetype>
          </prop>
      </propstat>
  </response>
  <response>
      <href>dcbox:/ServiceInfo/ServiceInfo.json</href>
      <propstat>
          <prop>
              <getcontenttype>application/json</getcontenttype>
          </prop>
      </propstat>
